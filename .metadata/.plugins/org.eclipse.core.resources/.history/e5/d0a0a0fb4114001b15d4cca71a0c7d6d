
public class Zoo {
	
	public static void main(String args[]) {
		
		Animal animal1 = new Animal(12, "M", 23);
		
		animal1.eat();
		
		Chicken chick1 = new Chicken(1, "M", 7);
		
		Bird bird1 = new Bird(2, "F", 4);
		
		Fish fish1 = new Fish();
		
		bird1.fly();
		fish1.swim();
		
		chick1.fly();
	}

}
